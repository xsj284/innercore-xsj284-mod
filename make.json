{
    "info": {
        "name": "xsj284's mod",
        "author": "xsj284",
        "version": "1.0-dev.1",
        "description": "",
        "icon": "mod_icon.png",
        "clientOnly": false
    },
    "api": "CoreEngine",
    "resources": [
        {
            "path": "assets/res",
            "type": "resource_directory"
        },
        {
            "path": "assets/gui",
            "type": "gui"
        }
    ],
    "sources": [
        {
            "source": "dev",
            "target": "main.js",
            "type": "main",
            "language": "typescript"
        },

        {
            "source": "lib/*",
            "type": "library",
            "language": "typescript"
        },

        {
            "source": "preloader",
            "type": "preloader",
            "language": "typescript"
        },
        {
            "source": "launcher",
            "type": "launcher",
            "language": "typescript"
        }
    ],
    "compile": [
        {
            "source": "java/*",
            "type": "java"
        },
        {
            "source": "native/*",
            "type": "native"
        }
    ],
    "additional": [
        {
            "source": "assets/root/*",
            "targetDir": "."
        }
    ]
}
